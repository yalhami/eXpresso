    font-size: 11px;
    font-weight: normal;
    color: #3A2B18;
    text-decoration: none;
    background-color: #fff;
    vertical-align: MIDDLE;
    padding-top: 5px;
    padding-right: 5px;
    padding-bottom: 6px;
    padding-left: 5px;
text-align:right;
}
.PollContainer
{
    font-family: tahoma;
    font-size: 12px;
    color: #3A2B18;
    text-decoration: none;
    background-color: #dedbcb;
    width: 360px;
    border: 1px solid #D1CFC7;
    padding-top: 10px;
    padding-right: 20px;
    padding-bottom: 20px;
    padding-left: 20px;
    height: 260px;
}
.PollStatusBar
{
    width: 110px;
    background-image: url(images/resbar.png);
    padding: 3px;
    height: 12px;
    margin: 2px;
    font-family: tahoma;
    font-size: 9px;
    font-weight: bold;
    color: #FFF;
    text-align: left;
}
.PollTitle
{
    font-size: 14px;
    font-weight: bold;
    color: #d77a25;
    font-family: tahoma;
    text-decoration: none;
    float: right;
    width: 360px;
    height: 26px;
}
.PollClose
{
    font-size: 14px;
    font-weight: bold;
    color: #d77a25;
    font-family: tahoma;
    text-decoration: none;
    float: right;
    width: 360px;
    text-align: left;
}
.PollResultsArea
{
    font-size: 12px;
    font-weight: normal;
    color: #5e5e5e;
    font-family: tahoma;
    text-decoration: none;
    float: right;
    width: 360px;
    height: 26px;
}
.backModal
{
    background-color: Gray;
    opacity: 0.1;
    filter: alpha(opacity=15);
}

.gallery_group_view
{
    float: right;
    padding: 0 20px 20px 0;
}
.gallery_group_view .img_cat img
{
    width: 132px;
    height: 73px;
    border: 0;
}
.gallery_group_view .img_cat img:hover
{
    opacity: 0.7;
    filter: alpha(opacity=70);
    background: #fff;
}
.gallery_group_view .img_cat
{
    border: solid 1px #d7d7d6;
    display: block;
    padding: 3px;
}
.gallery_group_view .img_cat:hover
{
    padding: 0;
    border: solid 4px #0092dd;
}
.gallery_group
{
    padding-top: 20px;
}
.pager
{
    width: 100%;
    background: #f0f0f0;
}
.pager .btn_pre
{
    padding: 4px;
    text-align: right;
}
.pager .btn_next
{
    padding: 4px;
    text-align: left;
}
.pager .btn_pre a
{
    background: url(../../../upload/files/editorimage/pager.png) 0 -22px no-repeat;
    height: 22px;
    width: 22px;
    display: block;
}
.pager .btn_next a
{
    background: url(../../../upload/files/editorimage/pager.png) -22px -22px no-repeat;
    height: 22px;
    width: 22px;
    display: block;
    float: left;
}
.pager .btn_pre a:link
{
    background: url(../../../upload/files/editorimage/pager.png) 0 0 no-repeat;
    height: 22px;
    width: 22px;
    display: block;
    cursor: pointer;
}
.pager .btn_next a:link
{
    background: url(../../../upload/files/editorimage/pager.png) -22px 0 no-repeat;
    height: 22px;
    width: 22px;
    display: block;
    cursor: pointer;
    float: left;
}

.cal_Theme1 .ajax__calendar_container
{
    background-color: #e2e2e2;
    border: solid 1px #cccccc;
    z-index:1000;
}

.cal_Theme1 .ajax__calendar_header
{
    background-color: #ffffff;
    margin-bottom: 4px;
}

.cal_Theme1 .ajax__calendar_title, .cal_Theme1 .ajax__calendar_next, .cal_Theme1 .ajax__calendar_prev
{
    color: #F60;
    padding-top: 3px;
}

.cal_Theme1 .ajax__calendar_body
{
    background-color: #e9e9e9;
    border: solid 1px #cccccc;
}

.cal_Theme1 .ajax__calendar_dayname
{
    text-align: center;
    font-weight: bold;
    margin-bottom: 4px;
    margin-top: 2px;
}

.cal_Theme1 .ajax__calendar_day
{
    text-align: center;
}

.cal_Theme1 .ajax__calendar_hover .ajax__calendar_day, .cal_Theme1 .ajax__calendar_hover .ajax__calendar_month, .cal_Theme1 .ajax__calendar_hover .ajax__calendar_year, .cal_Theme1 .ajax__calendar_active
{
    color: #F60;
    font-weight: bold;
    background-color: #ffffff;
}

.cal_Theme1 .ajax__calendar_today
{
    font-weight: bold;
}

.cal_Theme1 .ajax__calendar_other, .cal_Theme1 .ajax__calendar_hover .ajax__calendar_today, .cal_Theme1 .ajax__calendar_hover .ajax__calendar_title
{
    color: #bbbbbb;
}
